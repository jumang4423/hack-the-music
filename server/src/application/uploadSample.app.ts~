import { Sample } from "../generated/graphql";

export const UploadSampleApp = async (
  sample: Sample
): Promise<[Theme | null, ErrStr]> => {
  const [themeGlobalLenCached, err] = await GetThemeLenDriver();
  if (err.IsError()) {
    return [null, err];
  }
  const [_, err2] = await InsertThemeDriver(theme, themeGlobalLenCached);
  if (err2.IsError()) {
    return [null, err];
  }
  return [theme, new ErrStr({ isErr: false })];
};
